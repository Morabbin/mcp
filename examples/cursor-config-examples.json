{
  "mcpServers": {
    "haskell-stdio-server-dev": {
      "command": "cabal",
      "args": ["run", "--project-dir", "/Users/username/projects/mcp-haskell", "-v0", "mcp-stdio", "--", "--log"],
      "cwd": "/Users/username/projects/mcp-haskell",
      "env": {
        "GHC_ENVIRONMENT": "-"
      }
    },
    "haskell-stdio-server-prod": {
      "command": "/path/to/bin/mcp-stdio",
      "args": ["--log"],
      "cwd": "/path/to/system/lib/mcp",
      "env": {
        "MCP_TOOLS_PATH": "/path/to/system/share/mcp-tools",
        "MCP_RESOURCES_PATH": "/path/to/system/lib/mcp/resources"
      }
    }
  }
}
